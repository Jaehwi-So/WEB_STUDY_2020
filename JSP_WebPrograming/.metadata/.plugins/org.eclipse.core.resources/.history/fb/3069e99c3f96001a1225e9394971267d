<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!-- 구구단을 테이블 형태로 웹상에 보여지게 하기 -->
<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>Insert title here</title>
		<style>
		 td{
			 border : solid black;
		 }
		</style>
	</head>


	<body>
		<table>
			<% for(int i = 1; i <= 9; i++){ %> <tr>
				<% for(int j = 2; j <= 9; j++) { %> <td> 
					<%= j + "*" + i + "=" + i * j %> </td>
				<% } %> <tr>
			<% } %>

		</table>
	</body>
</html>