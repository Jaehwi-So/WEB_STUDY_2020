package vo;

//변수명과 컬럼명을 똑같이 만드는 것이 좋다.
public class DeptVO {
	private int deptNO;
	private String dName;
	private String loc;
	
	public int getDeptNO() {
		return deptNO;
	}
	public void setDeptNO(int deptNO) {
		this.deptNO = deptNO;
	}
	public String getdName() {
		return dName;
	}
	public void setdName(String dName) {
		this.dName = dName;
	}
	public String getLoc() {
		return loc;
	}
	public void setLoc(String loc) {
		this.loc = loc;
	}
	
}
